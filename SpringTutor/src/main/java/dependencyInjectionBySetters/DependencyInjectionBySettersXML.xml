<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="emp1" class="dependencyInjectionBySetters.Employee">
		<property name="id"><value>2</value></property>
		<property name="name"><value>Manju</value></property>
	</bean>
	<bean id="adrs" class="dependencyInjectionBySetters.Address">
		<property name="city" value="Coimbatore"/>
		<property name="state" value="TN"/>
		<property name="country" value="India"/>
	</bean>	
	<bean id="emp2" class="dependencyInjectionBySetters.Employee">
		<property name="id" value="2"/>
		<property name="name" value="Manju"/>
		<property name="address" ref="adrs"/>
	</bean>
	<bean id="emp3" class="dependencyInjectionBySetters.Employee">
		<property name="id" value="2"/>
		<property name="name" value="Manju"/>
		<property name="address" ref="adrs"/>
		<property name="skillSet">
			<list>
				<value>Java</value>
				<value>C++</value>
				<value>DBMS</value>
			</list>
		</property>
	</bean>
	<bean id="proj1" class="dependencyInjectionBySetters.Projects">
		<property name="projectID" value="123"/>
		<property name="projectName" value="Target"/>
	</bean>
	<bean id="proj2" class="dependencyInjectionBySetters.Projects">
		<property name="projectID" value="789"/>
		<property name="projectName" value="COE"/>
	</bean>
	<bean id="emp4" class="dependencyInjectionBySetters.Employee">
		<property name="id" value="2"/>
		<property name="name" value="Manju"/>
		<property name="address" ref="adrs"/>
		<property name="skillSet">
			<list>
				<value>Java</value>
				<value>C++</value>
				<value>DBMS</value>
			</list>
		</property>
		<property name="projectsWorked">
			<list>  
				<ref bean="proj1"/>  
				<ref bean="proj2"/>
			</list>
		</property>		
	</bean>	
	<bean id="emp5" class="dependencyInjectionBySetters.Employee">
		<property name="id" value="2"/>
		<property name="name" value="Manju"/>
		<property name="address" ref="adrs"/>
		<property name="skillSet">
			<list>
				<value>Java</value>
				<value>C++</value>
				<value>DBMS</value>
			</list>
		</property>
		<property name="projectsWorked">
			<list>  
				<ref bean="proj1"/>  
				<ref bean="proj2"/>
			</list>
		</property>	
		<property name="projectTechnology">
			<map>
				<entry key-ref="proj1" value="Spring MVC"/>
				<entry key-ref="proj2" value="Java"/>
			</map>
		</property>	
	</bean>
</beans>